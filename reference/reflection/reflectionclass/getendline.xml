<refentry xml:id="reflectionclass.getendline" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>ReflectionClass::getEndLine</refname>
  <refpurpose>Pobiera numer ostatniej linii</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="ReflectionClass">
   <modifier>public</modifier> <type class="union"><type>int</type><type>false</type></type><methodname>ReflectionClass::getEndLine</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Pobiera numer ostatniej linii klasy zdefiniowanej przez użytkownika.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Numer ostatniej linii klasy zdefiniowanej przez użytkownika lub &false; jeśli jest on nieznany.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Przykład użycia <methodname>ReflectionClass::getEndLine</methodname></title>
    <programlisting role="php">
<![CDATA[
<?php
// Klasa testowa
class Klasa { }

$rc = new ReflectionClass('Klasa');

echo $rc->getEndLine();
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
3
]]>
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>ReflectionClass::getStartLine</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>
